{
    "extends": ["next", "prettier"],
    "rules": {
        "quotes": ["error", "single", { "avoidEscape": true }],
        "max-len": ["error", 120],
        "semi": ["error", "always"],
        "jsx-quotes": ["error", "prefer-single"],
        "no-else-return": 2,
        "space-unary-ops": 2
    }
}
